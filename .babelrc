{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": { "version": 3, "proposals": true }
      }
    ],
    "@babel/preset-react"
  ],
  // "plugins": ["react-hot-loader/babel"] -> no xq estoy solo con HMR
  "plugins": [
    "add-module-exports",
    ["@babel/plugin-transform-runtime", { "corejs": 3, "proposals": true }],
    "@babel/plugin-syntax-dynamic-import",
    "@loadable/babel-plugin",
    "dynamic-import-node"
  ]
}
